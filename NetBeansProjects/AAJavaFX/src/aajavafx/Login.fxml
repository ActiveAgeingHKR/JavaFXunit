<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="300.0" prefWidth="402.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="aajavafx.LoginController">
    <children>
        <Button id="login" fx:id="button" layoutX="164.0" layoutY="237.0" onAction="#handleButtonLoginAction" text="Login" />
        <Label id="outputmessageID" fx:id="outputmessageID" layoutX="156.0" layoutY="209.0" minHeight="16" minWidth="69" />
      <TextField id="userID" fx:id="userID" layoutX="107.0" layoutY="99.0" onAction="#handleButtonLoginAction" promptText="Username" />
      <PasswordField id="passwordID" fx:id="passwordID" layoutX="107.0" layoutY="137.0" onAction="#handleButtonLoginAction" promptText="Password" />
      <CheckBox id="saveCredentials" fx:id="saveCredentials" layoutX="106.0" layoutY="176.0" mnemonicParsing="false" text="Save Credentials" />

    </children>
</AnchorPane>
